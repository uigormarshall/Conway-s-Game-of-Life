{
  "compilerOptions": {                     
    "target": "es6",
    "module": "ES6",
    "outDir": "./public/js",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true ,
    "sourceRoot": "../src" ,
    "inlineSourceMap": true ,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
  }
}
